<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myapp.FLogin">
   <children>
      <ImageView fitHeight="400.0" fitWidth="767.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/Login.png" />
         </image>
      </ImageView>
      <TextField fx:id="tfName" layoutX="480.0" layoutY="166.0" prefHeight="19.0" prefWidth="155.0" promptText="Username" style="-fx-background-color: transparant;" />
      <PasswordField fx:id="tfPassword" layoutX="480.0" layoutY="211.0" prefHeight="26.0" prefWidth="155.0" promptText="Password" style="-fx-background-color: transparant;" />
      <Button fx:id="btnLogin" layoutX="497.0" layoutY="265.0" mnemonicParsing="false" onAction="#btnLogin" prefHeight="39.0" prefWidth="114.0" style="-fx-background-color: transparant; -fx-text-fill: transparant;" text="Button" textFill="TRANSPARENT" />
   </children>
</AnchorPane>
