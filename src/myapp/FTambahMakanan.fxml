<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="759.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="myapp.FTambahMakanan">
   <children>
      <ImageView fitHeight="400.0" fitWidth="768.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@img/Tambah%20Menu.png" />
         </image>
      </ImageView>
      <TextField fx:id="txtHarga" layoutX="255.0" layoutY="184.0" prefHeight="26.0" prefWidth="222.0" style="-fx-background-color: FFFFFFF;">
         <font>
            <Font name="Inter Regular" size="12.0" />
         </font>
         <effect>
            <Blend mode="SCREEN" />
         </effect>
      </TextField>
      <TextField fx:id="txtNama" layoutX="253.0" layoutY="132.0" prefHeight="23.0" prefWidth="214.0" style="-fx-background-color: FFFFFF;">
         <font>
            <Font name="Inter Regular" size="12.0" />
         </font>
      </TextField>
      <Button fx:id="btTambah" layoutX="629.0" layoutY="284.0" mnemonicParsing="false" onAction="#tambahMakanan" prefHeight="26.0" prefWidth="86.0" style="-fx-background-color: 0px;" />
      <Button fx:id="btClear" layoutX="627.0" layoutY="318.0" mnemonicParsing="false" onAction="#clear" prefHeight="26.0" prefWidth="86.0" style="-fx-background-color: 0px;" />
      <Button fx:id="btKeluar" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#keluar" prefHeight="26.0" prefWidth="29.0" style="-fx-opacity: 0%;" text="Button">
         <font>
            <Font size="6.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
